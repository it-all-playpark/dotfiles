{
  "mcpServers": {
    "c7": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@upstash/context7-mcp",
        "--key",
        "${SMITHERY_API_KEY}",
        "--profile",
        "${SMITHERY_PROFILE}"
      ]
    },
    "gh": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
        "run",
        "@smithery-ai/github",
        "--key",
        "${SMITHERY_API_KEY}",
        "--profile",
        "${SMITHERY_PROFILE}"
      ]
    },
    "seq": {
      "command": "npx",
      "args": [
        "-y",
        "@smithery/cli@latest",
